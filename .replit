modules = ["php-8.2", "nodejs-20", "web", "postgresql-16"]
[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["imagemagick", "grpc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Question API"

[[workflows.workflow]]
name = "Backend"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php -S 0.0.0.0:5000 -t public"
waitForPort = 5000

[[workflows.workflow]]
name = "Question API"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node question-api.js"
waitForPort = 3000

[workflows.workflow.metadata]
outputType = "console"

